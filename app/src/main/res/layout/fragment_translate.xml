<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context="com.example.maryjoy.northerdialecttranslator.translateFragment"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">
     />

    <EditText
        android:id="@+id/editText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="17dp"
        android:layout_marginStart="17dp"
        android:background="@drawable/back"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Enter text"
        android:textColor="#000000"
        android:textColorHint="#000000"
        android:textSize="15dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/spinner1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="58dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="69dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/one"
        android:layout_marginLeft="17dp"
        android:layout_marginRight="17dp" />

    <Button
        android:id="@+id/btntranslate"
        android:layout_width="140dp"
        android:layout_height="0dp"
        android:background="@drawable/back"
        android:text="Translate"
        android:textColor="#000000"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imgbtn1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/resultText"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/imgbtn1"
        android:layout_width="64dp"
        android:layout_height="0dp"
        android:layout_marginEnd="87dp"
        android:background="@drawable/back"
        app:srcCompat="@drawable/play_50px"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtn2"
        app:layout_constraintRight_toRightOf="@+id/btntranslate"
        app:layout_constraintTop_toTopOf="@+id/imgbtn2"
        android:layout_marginRight="87dp" />

    <ImageButton
        android:id="@+id/imgbtn2"
        android:layout_width="65dp"
        android:layout_height="60dp"
        android:layout_marginEnd="29dp"
        android:layout_marginStart="22dp"
        android:background="@drawable/back"
        app:srcCompat="@drawable/copy_50px"
        app:layout_constraintRight_toLeftOf="@+id/imgbtn3"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="58dp"
        app:layout_constraintLeft_toRightOf="@+id/imgbtn1"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="29dp" />

    <ImageButton
        android:id="@+id/imgbtn3"
        android:layout_width="65dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/back"
        app:srcCompat="@drawable/trash_can_50px"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtn2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgbtn2"
        android:layout_marginRight="16dp" />

    <ImageButton
        android:id="@+id/imgbtn4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/back"
        app:srcCompat="@drawable/star_32"
        app:layout_constraintRight_toLeftOf="@+id/imgbtn1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imgbtn1"
        android:layout_marginEnd="18dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imgbtn1"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="18dp" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@drawable/back"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="65dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="203dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="65dp"
        android:layout_marginRight="65dp" />

    <TextView
        android:id="@+id/resultText"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:background="@drawable/back"
        android:text="Get words.."
        android:textColor="#000000"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/editText"
        android:layout_marginTop="77dp"
        app:layout_constraintTop_toBottomOf="@+id/editText"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/editText" />

    <ImageView
        android:id="@+id/one"
        android:layout_width="0dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/cheap_32px"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/btnspeak"
        android:layout_marginTop="25dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/btnspeak"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/two"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="2dp"
        android:layout_marginStart="2dp"
        app:layout_constraintBottom_toTopOf="@+id/resultText"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/spinner1"
        app:layout_constraintTop_toTopOf="@+id/spinner1"
        app:srcCompat="@drawable/circled_2_32px"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:background="@drawable/back"
        android:id="@+id/btnspeak"
        android:layout_width="58dp"
        android:layout_height="0dp"
        app:srcCompat="@drawable/microphone_96px"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/editText"
        android:layout_marginEnd="15dp"
        app:layout_constraintRight_toRightOf="@+id/editText"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintTop_toTopOf="@+id/editText"
        android:layout_marginRight="15dp" />

</android.support.constraint.ConstraintLayout>
